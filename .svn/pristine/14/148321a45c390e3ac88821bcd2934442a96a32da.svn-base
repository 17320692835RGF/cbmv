body {
	margin: 0;
	padding: 0;
}
ul {
	list-style: none;
	margin: 0;
	padding: 0;
}
a{
	text-decoration: none;
}

.cbmv-header {
	width: 100%;
	height: 80px;
	background-color: #292a2a;
	position: fixed;
	top: 0;
	z-index: 999;
}

.cbmv-header-left {
	margin-left: 15px;
	margin-top: 5px;
}
.cbmv-header-right {
	margin-right: 15px;
	margin-top: 5px;
}
.cbmv-header-right a {
	padding-left: 10px;
}
.cbmv-header-right a,.cbmv-header-left a {
	color: white;
}
.more-icon {
	color: white;
}
.search-icon {
	color: white;
	margin-left: 10px;
}

.flex-container {
	display: flex;
	flex-flow: row wrap;
	justify-content: space-between;
}

.navbar-header {
	background-color: #292a2a;
}
.nav-container {
	width: 100px;
	background-color: rgba(0, 0, 0, 0.9);
	float: right;
}

.cbmv-top {
	width: 100%;
	height: 90px;
	margin-top: 20px;
}
.cbmv-type1 {
	width: 100%;
	height: 35px;
	margin-top: 5px;
	border-bottom:1px solid darkgray;
}
.cbmv-type2 {
	width: 100%;
	height: 60px;
	margin-top: 5px;
	border-bottom:1px solid darkgray;
}
.cbmv-type1-left {
	width: 20%;
	line-height: 30px;
	float: left;
}
.cbmv-type1-right {
	width: 79%;
	line-height: 30px;
	float: right;
}
.cbmv-type1-right a {
	color: #292a2a;
}
.cbmv-type1-font {
	color: #337ab7;
	font-weight: 700;
	margin-left: 20px;;
}

.cbmv-type1-last {
	margin-top: 10px;
}
.cbmv-type1-last span{
	color: #292a2a;
	font-weight: 700;
	margin-left: 20px;;
}
.cbmv-videos {
	height: 1150px;
	margin-top: 12px;
}
.cbmv-video1 {
	margin-top: 10px;
	padding-left: 10px;
	padding-right: 10px;
	padding-top: 10px;
	padding-bottom: 10px;
	height: 260px;
	border: 1px solid gainsboro;
	box-shadow: 2px 4px 3px darkgrey;
	position: relative;
}
.glyphicon-star,.video-desc {
	color: orangered;
}
.video-title {
	text-align: center;
	font-size: 18px;
	font-weight: 700;
	border-bottom:1px solid darkgray;
}
.pagination {
	float: right;
}

.cbmv-footer {
	height: 140px;
	background-color: #292a2a;
}
.cbmv-cpinfo {
	margin-top: 10px;
	color: white;
}
.cbmv-cpinfo-left {
	font-size: 18px;
	font-weight: 700;
}
.cbmv-footer-fixed {
	width: 100%;
	height: 45px;
	line-height: 40px;
	color: white;
	position: fixed;
	bottom: 0;
	background-color: #158aeb;
}

.actives {
	color: white;
	border-radius: 6px 6px 6px 6px;
	background-color: #337ab7;
	text-decoration: none;
}
.video-class {
	width: 40px;
	height: 20px;
	color: white;
	text-align: center;
	border-radius: 4px 4px 4px 4px;
	background-color: #337ab7;
}

.cbmv-videos dl dd:last-of-type {
	margin-top: 5px;
}
.navbar-nav span {
	color: white;
}
.fixed-nav-right {
	position: fixed;
	right: 0;
	min-width: 23%;
	background-color: rgba(0, 0, 0, 0.9);
	display: none;
}
.fixed-nav-right a {
	line-height: 35px;
	color: white;
	margin-left: 16px;
}
.fixed-nav-helps {
	position: fixed;
	left: 0;
	min-width: 100%;
	line-height: 30px;
	background-color: rgba(0, 0, 0, 0.9);
	display: none;
}
.fixed-nav-helps a {
	margin-left: 20px;
	color: white;
	opacity: 0.7;
}
.fixed-nav-search {
	position: fixed;
	left: 0;
	min-width: 100%;
	background-color: rgba(0, 0, 0, 0.9);
	display: none;
	text-align: center;
}
.video-play-btn {
	position: absolute;
	top:80px;
	left:50%;
	border:1px,solid,red;
}
.pagination {
	margin-top: -55px;
}
.cbmv-slide {
	width:100%;
	height:182px;
	margin-top: 85px;
}
#carousel-example-generic {
	border: 1px solid #dcdcdc;
}
.carousel-control.left{
	background-image:none;        
    background-repeat: repeat-x; 
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#80000000', endColorstr='#00000000', GradientType=1); 
}
.carousel-control.right {
    left: auto; 
    right: 0; 
    background-image:none;
    background-repeat: repeat-x;
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#00000000', endColorstr='#80000000', GradientType=1);          
}
.my-slide-show {
	padding: 10px 10px 10px 10px;
}